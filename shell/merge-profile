#!/bin

cur_path="${PWD}/$(dirname $0)"
source ${cur_path}/../lib/tool.sh

os="$(tool::os_type)"

while read line; do 
    if grep -q "$line" ~/.bash_profile ; then
        echo "W exists $line"
    else
        echo "I append $line"
        echo "$line" >> ~/.bash_profile
    fi
done < ${cur_path}/bash_profile_mac

